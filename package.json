{
  "name": "sircus-tools-display-responsive",
  "description": "A responsive display tools Module for Sircus.",
  "version": "0.0.1",
  "homepage": "http://sircus.github.io",
  "keywords": [
    "scalability",
    "maintainability",
    "designability",
    "css"
  ],
  "author": {
    "name": "Yasuyuki Enomoto",
    "url": "http://blivesta.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/sircus/tools-display-responsive.git"
  },
  "scripts": {
    "build:base": "css2scss ./lib/base.css ./scss/_base.scss",
    "build:sm": "css2scss ./lib/sm.css ./scss/_sm.scss",
    "build:md": "css2scss ./lib/md.css ./scss/_md.scss",
    "build:lg": "css2scss ./lib/lg.css ./scss/_lg.scss",
    "build": "npm run build:base && npm run build:sm && npm run build:md && npm run build:lg"
  },
  "dependencies": {
    "sircus-global-property": "^0.0.1"
  },
  "devDependencies": {
    "sircus-process-css2scss": "^0.2.0"
